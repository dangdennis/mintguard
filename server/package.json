{
  "name": "mintguard-server",
  "version": "1.0.0",
  "description": "",
  "main": "dist/server.js",
  "scripts": {
    "build": "esbuild src/server.ts --bundle --platform=node --outfile=dist/server.js --external:./node_modules/*",
    "build:dev": "esbuild src/server.ts --bundle --platform=node --outfile=dist/server.js --external:./node_modules/* --watch",
    "start:dev": "nodemon dist/server.js",
    "start": "node dist/server.js",
    "format": "prettier --write ./src/**/*",
    "tsc:dev": "npx tsc --watch",
    "prisma": "prisma"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^3.9.1",
    "@types/http-errors": "1.8.2",
    "@types/morgan": "1.9.3",
    "@types/pg": "8.6.4",
    "dotenv": "14.2.0",
    "express": "4.17.2",
    "http-errors": "2.0.0",
    "morgan": "1.10.0",
    "pg": "8.7.1"
  },
  "devDependencies": {
    "@types/express": "4.17.13",
    "@types/node": "17.0.10",
    "esbuild": "0.14.18",
    "nodemon": "2.0.15",
    "prettier": "2.5.1",
    "prisma": "3.9.1",
    "typescript": "4.5.4"
  }
}